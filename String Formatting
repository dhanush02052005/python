def print_formatted(number):
    # your code goes here
    width = len(bin(number)[2:])
    for i in range(1,number+1):
        decimal = str(i)
        octal = oct(i)[2:]
        hexa_decimal = hex(i)[2:].upper()
        binary = bin(i)[2:]
        print(decimal.rjust(width),octal.rjust(width),hexa_decimal.rjust(width),binary.rjust(width))
